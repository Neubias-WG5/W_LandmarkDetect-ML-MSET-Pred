{
    "name": "LandmarkDetect-ML-MSET-Pred-A",
    "container-image": {
        "image": "cytomineuliege/s_landmarkdetect-ml-mset-pred",
        "type": "singularity"
    },
    "schema-version": "cytomine-0.1",
    "description": "optional description",
    "command-line": "python run.py CYTOMINE_HOST CYTOMINE_PUBLIC_KEY CYTOMINE_PRIVATE_KEY CYTOMINE_ID_PROJECT CYTOMINE_ID_SOFTWARE IMAGES_TO_PREDICT MODEL_TO_USE",
    "inputs": [
        {
        	"id": "cytomine_host",
      		"value-key": "@ID",
      		"command-line-flag": "--@id",
      		"name": "Cytomine host",
      		"set-by-server": true,
      		"optional": false,
      		"type": "String"
    	},
        {
			"id": "cytomine_public_key",
			"value-key": "@ID",
			"command-line-flag": "--@id",
			"name": "Cytomine public key",
			"set-by-server": true,
			"optional": false,
			"type": "String"
    	},
    	{
    		"id": "cytomine_private_key",
			"value-key": "@ID",
			"command-line-flag": "--@id",
			"name": "Cytomine private key",
			"set-by-server": true,
			"optional": false,
			"type": "String"
    	},
		{
			"id": "cytomine_id_project",
			"value-key": "@ID",
			"command-line-flag": "--@id",
			"name": "Cytomine project ID",
			"set-by-server": true,
			"optional": false,
			"type": "Number"
    	},
		{
			"id": "cytomine_id_software",
			"value-key": "@ID",
			"command-line-flag": "--@id",
			"name": "Cytomine software ID",
			"set-by-server": true,
			"optional": false,
			"type": "Number"
		},
        {
            "id": "images_to_predict",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "Images on which to apply the model",
            "set-by-server": false,
            "optional": false,
            "type": "ListDomain",
	        "uri": "/api/project/$currentProject$/imageinstance.json",
	        "uri-print-attribute": "instanceFilename",
	        "uri-sort-attribute": "instanceFilename"
        },
        {
            "id": "model_to_use",
            "value-key": "@ID",
            "command-line-flag": "--@id",
            "name": "CytomineJob ID of the model to use",
            "set-by-server": false,
            "optional": false,
            "type": "Domain",
	    	"uri": "/api/job.json?project=$currentProject$",
	    	"uri-print-attribute": "id",
	    	"uri-sort-attribute": "id"
        }
    ]
}